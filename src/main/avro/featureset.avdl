@namespace("it.crs4.features")
protocol FeatureSet {

  record Signatures {
    //-- from WND-CHARM --
    array<string> feature_names;
    array<double> values;
    union { null, string } name;
    union { null, string } feature_set_version;
    union { null, string } source_filepath;
    union { null, string } auxiliary_feature_storage;
    //-- additional --
    union { null, string } plane_tag;
  }

}

